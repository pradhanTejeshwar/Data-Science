public class Main {
	public static void main(String[] args) {
		String[] str1=new String[] {"apple", "orange", "grapes"};
		int index=0;
		String[] str2=new String[];
		for(String temp:str1) {
			char ch=temp.charAt(0);
			if(ch=='a' || ch=='g')
				str2[index++]=temp;
			System.out.println(ch+",");
		}
			
	}
}
