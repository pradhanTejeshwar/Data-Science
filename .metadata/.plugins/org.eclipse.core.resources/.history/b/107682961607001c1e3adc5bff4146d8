import java.util.Scanner;

public class Q13Main {

	public static void main(String[] args) {
		int input1[5] = null;
		int input2[] = null;
		int output[] = null;
		System.out.println("Enter 5 Value for Array 1: ");
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<5;i++) {
			input1[i]=sc.nextInt();
		}
		System.out.println("Enter 5 Value for Array 2: ");
		for(int i=0;i<5;i++) {
			input2[i]=sc.nextInt();
		}
		
		System.arraycopy(input1, 0, output, 0, 1);
		System.arraycopy(input2, 1, output, 1, 1);
		System.arraycopy(input1, 2, output, 2, 1);
		System.arraycopy(input2, 3, output, 3, 1);
		System.arraycopy(input1, 4, output, 4, 1);
		
		for(int temp:output)
			System.out.print(temp+",");
		
	}

}
